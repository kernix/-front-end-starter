{
  "name": "framework-ui",
  "description": "A light framework aimed for use to Front-End Developers",
  "dependencies": {
    "bootstrap": "~3.3.7",
    "font-awesome": "~4.7.0",
    "jquery": "~3.1.1",
    "hammerjs": "hammer.js#~2.0.6",
    "eonasdan-bootstrap-datetimepicker": "~4.17.45",
    "moment": "~2.17.0",
    "jquery-placeholder": "~2.3.1",
    "sweetalert": "~1.1.3",
    "aos": "^2.1.1",
    "material-design-iconic-font": "^2.2.0",
    "rellax": "*"
  },
  "overrides": {
    "sweetalert": {
      "main": [
        "./dist/sweetalert.min.js"
      ]
    },
    "jquery-placeholder": {
      "main": [
        "./jquery.placeholder.min.js"
      ]
    },
    "hammerjs": {
      "main": [
        "./hammer.min.js",
        "./hammer.js"
      ]
    },
    "moment": {
      "main": [
        "./min/moment.min.js",
        "./locale/fr.js"
      ]
    },
    "eonasdan-bootstrap-datetimepicker": {
      "main": [
        "./build/js/bootstrap-datetimepicker.min.js"
      ]
    },
    "bootstrap": {
      "main": [
        "./dist/js/bootstrap.min.js",
        "./dist/fonts/*"
      ]
    },
    "font-awesome": {
      "main": [
        "./css/font-awesome.min.css",
        "./fonts/fontawesome-webfont.*"
      ]
    },
    "material-design-iconic-font": {
      "main": [
        "./dist/css/material-design-iconic-font.min.css",
        "./dist/fonts/*"
      ]
    },
    "aos": {
      "main": [
        "./dist/aos.js",
        "./dist/aos.css"
      ]
    },
    "jquery": {
      "main": [
        "./dist/jquery.min.js"
      ]
    }
  }
}
