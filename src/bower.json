{
  "name": "framework-ui",
  "description": "A light framework aimed for use to Front-End Developers",
  "dependencies": {
    "bootstrap": "~3.3.6",
    "font-awesome": "~4.6.3",
    "jquery": "~2.2.2",
    "hammerjs": "hammer.js#~2.0.6",
    "eonasdan-bootstrap-datetimepicker": "~4.17.37",
    "moment": "~2.13.0",
    "jquery-placeholder": "~2.3.1",
    "sweetalert": "~1.1.3"
  },
  "overrides": {
    "sweetalert": {
      "main": [
        "./dist/sweetalert.min.js"
      ]
    },
    "jquery-placeholder": {
      "main": [
        "./jquery.placeholder.min.js"
      ]
    },
    "hammerjs": {
      "main": [
        "./hammer.min.js",
        "./hammer.js"
      ]
    },
    "moment": {
      "main": [
        "./min/moment.min.js",
        "./locale/fr.js"
      ]
    },
    "eonasdan-bootstrap-datetimepicker": {
      "main": [
        "./build/js/bootstrap-datetimepicker.min.js"
      ]
    },
    "bootstrap": {
      "main": [
        "./dist/js/bootstrap.min.js",
        "./dist/fonts/*"
      ]
    },
    "font-awesome": {
      "main": [
        "./css/font-awesome.min.css",
        "./fonts/fontawesome-webfont.*"
      ]
    },
    "jquery": {
      "main": [
        "./dist/jquery.min.js"
      ]
    }
  },
  "resolutions": {
    "font-awesome": "~4.6.3",
    "moment": "~2.13.0"
  }
}
